  "paths": {
    "/api/shopify-integration/orders": {
      "get": {
        "summary": "GetOrdersByUser returns a list of shopify orders filtered by given email, this API is only open for global admin user",
        "operationId": "GetOrdersByUser",
        "responses": {
          "200": {
            "description": "A successful response.",
            "schema": {
              "$ref": "#/definitions/appserver_serves_uiGetOrdersByUserResponse"
            }
          },
          "default": {
            "description": "An unexpected error response.",
            "schema": {
              "$ref": "#/definitions/runtimeError"
            }
          }
        },
        "parameters": [
          {
            "name": "email",
            "description": "user's email address for supernode account.",
            "in": "query",
            "required": false,
            "type": "string"
          }
        ],
        "tags": [
          "ShopifyIntegration"
        ]
      }
    }
  },
  "definitions": {
    "appserver_serves_uiGetOrdersByUserResponse": {
      "type": "object",
      "properties": {
        "orders": {
          "type": "array",
          "items": {
